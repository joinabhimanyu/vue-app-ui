{
  "name": "drs_appui-client",
  "version": "0.0.0",
  "jest": {
    "automock": false,
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "testRegex": "(/ClientApp/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "<rootDir>/build/",
      "<rootDir>/config/",
      "<rootDir>/wwwroot/",
      "<rootDir>/node_modules/",
      "<rootDir>/.eslintrs.js",
      "<rootDir>/webpack.config.js"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ],
    "collectCoverage": false,
    "collectCoverageFrom": [
      "**/*.{js,vue}",
      "!**/node_modules/**"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/build/",
      "<rootDir>/config/",
      "<rootDir>/wwwroot/",
      "<rootDir>/node_modules/",
      "<rootDir>/.eslintrs.js",
      "<rootDir>/webpack.config.js"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text"
    ]
  },
  "scripts": {
    "dev": "node build/dev-server.js",
    "start": "node build/dev-server.js",
    "build": "webpack -p --config ./build/webpack.prod.conf.js",
    "test": "jest",
    "e2e": "node test/e2e/runner.js",
    "allTest": "npm run unit && npm run e2e",
    "lint": "eslint '**/*.{js,vue}' --quiet",
    "format": "prettier '**/*.{js,vue}' --write"
  },
  "dependencies": {
    "aspnet-webpack": "^1.0.25",
    "axios": "^0.15.3",
    "bootstrap": "4.0.0-beta",
    "chart.js": "^2.4.0",
    "element-ui": "^2.0.0",
    "font-awesome": "^4.7.0",
    "jquery": "^3.1.1",
    "lodash": "^4.17.4",
    "popper.js": "^1.12.6",
    "skeleton-css": "^2.0.4",
    "vue": "^2.1.0",
    "vue-context-menu": "^2.0.6",
    "vue-data-tables": "^2.1.1",
    "vue-i18n": "^7.2.0",
    "vue-lil-context-menu": "^1.1.0",
    "vue-material": "^0.4.1",
    "vue-router": "^2.1.1",
    "vuedraggable": "^2.15.0",
    "vuex": "^2.4.0",
    "vuex-router-sync": "^4.2.0",
    "webpack-hot-middleware": "^2.13.2",
    "webpack-node-externals": "^1.6.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "babel-core": "^6.0.0",
    "babel-eslint": "^7.0.0",
    "babel-jest": "^22.1.0",
    "babel-loader": "^6.0.0",
    "babel-plugin-component": "^0.10.1",
    "babel-plugin-import-noop": "^1.0.1",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-transform-runtime": "^6.0.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-stage-2": "^6.0.0",
    "babel-register": "^6.0.0",
    "chalk": "^1.1.3",
    "co": "^4.6.0",
    "connect-history-api-fallback": "^1.1.0",
    "css-loader": "^0.25.0",
    "es6-promise": "^4.1.1",
    "eslint": "^3.7.1",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-config-prettier": "^2.9.0",
    "eslint-friendly-formatter": "^2.0.5",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-html": "^1.3.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "eslint-plugin-vue": "^4.3.0",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^1.0.1",
